{
    "name": "tp/docker-composer",
    "autoload": {
        "psr-4": { "Docker\\": "src/Docker/" }
    },
    "autoload-dev" : {
        "psr-4": { "Docker\\Composer\\Tests\\" : "tests/Docker/Composer/Tests" }
    },
    "minimum-stability": "stable",
    "require": {
        "php"          : ">=5.3.0",
        "symfony/yaml" : "2.5.*",
        "marcj/topsort": "~0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit --coverage-clover build/coverage.xml"
    },
    "version": "1.0.1"
}
