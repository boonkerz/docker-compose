{
    "name": "tp/docker-composer",
    "description": "Parse Docker Composer Files",
    "autoload": {
        "psr-4": { "Docker\\": "src/Docker/" }
    },
    "autoload-dev" : {
        "psr-4": { "Docker\\Composer\\Tests\\" : "tests/Docker/Composer/Tests" }
    },
    "require": {
        "php"          : ">=5.3.0",
        "symfony/yaml" : "3.2.*",
        "marcj/topsort": "~0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit --coverage-clover build/coverage.xml"
    },
    "version": "1.1.2"
}
